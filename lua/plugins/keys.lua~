return {
  {
    'matze/vim-move',
    config = function()
      	require('vim-move').setup({
	    keys =  {
      			{ '<A-Down>', '<Plug>MoveLineDown', mode = { 'n', 'v' } },
      			{ '<A-Up>', '<Plug>MoveLineUp', mode = { 'n', 'v' } },
      			{ '<A-Right>', '<Plug>MoveHCharRight', mode = { 'n', 'v' } },
      			{ '<A-Left>', '<Plug>MoveHCharLeft', mode = { 'n', 'v' } },
    		},
     	})
	end,
  }
}
